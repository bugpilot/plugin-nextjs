{
  "name": "@bugpilot/plugin-nextjs",
  "version": "0.19.1",
  "description": "Bugpilot for Next.js App Router",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "prebuild": "pnpm run lint && pnpm audit",
    "cleanup": "rm -rf ./dist && mkdir ./dist",
    "compile": "tsc",
    "build": "npm run cleanup && npm run compile",
    "build:watch": "npx nodemon -w src -e ts -x pnpm -- build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugpilot/plugin-nextjs.git"
  },
  "keywords": [
    "bugpilot",
    "cli",
    "error",
    "error-monitoring",
    "react",
    "nextjs",
    "error-handling"
  ],
  "author": "Simone Lusenti <lusenti.s@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.7",
    "@babel/types": "^7.23.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@types/babel__core": "^7.20.5",
    "@types/babel-generator": "^6.25.8",
    "@types/babel-traverse": "^6.25.10",
    "@types/babel-types": "^7.0.15",
    "@types/node": "^20.11.5",
    "@types/react-dom": "^18.2.18",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint": "^8.56.0",
    "prettier-eslint": "^16.2.0",
    "prettier": "^3.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
